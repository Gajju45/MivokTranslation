<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mivok">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ghost"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ghost"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mivoktranslation.PharsesActivity"
            android:label="@string/category_phrases"
            android:parentActivityName="com.example.mivoktranslation.MainActivity">


        </activity>

        <activity android:name="com.example.mivoktranslation.FamilyActivity"
            android:label="@string/category_family"
            android:parentActivityName="com.example.mivoktranslation.MainActivity"/>





        <activity android:name="com.example.mivoktranslation.ColorsActivity"
            android:label="@string/category_colors"
            android:parentActivityName="com.example.mivoktranslation.MainActivity"/>




        <activity android:name="com.example.mivoktranslation.NumbersActivity"
            android:label="@string/category_numbers"
            android:parentActivityName="com.example.mivoktranslation.MainActivity"/>

        />



        <activity android:name="com.example.mivoktranslation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>